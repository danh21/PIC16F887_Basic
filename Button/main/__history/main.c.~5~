#include <main.h>

void main()
{  
   while(TRUE)
   {
      //TODO: User Code
      if (sensor1) {
         led1(on);
         buzzer1(on);
         delay_ms(1000);
      }
      
      if (sensor2) {
         led2(on);
         buzzer2(on);
      }
      
      if (rst) {
         led1(off);
         buzzer1(off);
         led2(off);
         buzzer2(off);
      }    
   }
}
