#include <main.h>

void main() {
   Data_Out(0x00);
   delay_ms(200);
   
   while(TRUE) {
      Data_Out(0b10000000);
      delay_ms(200);  
      
      Data_Out(0b11000000);
      delay_ms(200);
      
      Data_Out(0b11100000);
      delay_ms(200);
      
      Data_Out(0b11110000);
      delay_ms(200);
      
      Data_Out(0b11111000);
      delay_ms(200);
      
      Data_Out(0b11111100);
      delay_ms(200);
      
      Data_Out(0b11111110);
      delay_ms(200);
      
      Data_Out(0b11111111);
      delay_ms(200);
      
   }
}
