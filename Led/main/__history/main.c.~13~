#include <main.h>

void main() {
   Data_Out(0x00);
   delay_ms(200);
   
   while(TRUE) {
      Data_Out(0b00000001);
      delay_ms(200);  
      
      Data_Out(0b00000011);
      delay_ms(200);
      
      Data_Out(0b00000111);
      delay_ms(200);
      
      Data_Out(0b00001111);
      delay_ms(200);
      
      Data_Out(0b00011111);
      delay_ms(200);
      
      Data_Out(0b00111111);
      delay_ms(200);
      
      Data_Out(0b01111111);
      delay_ms(200);
      
      Data_Out(0b11111111);
      delay_ms(200);
      
   }
}
