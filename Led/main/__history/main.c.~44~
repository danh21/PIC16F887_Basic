#include <main.h>

void blink(int16 led, unsigned int16 ms) {
   output_high(led);   // LED on
   delay_ms(1000);  
   output_low(led);    // LED off
   delay_ms(1000);

//!   output_high(PIN_B0);
//!      delay_ms(1000);
//!      output_low(PIN_B0);
//!      delay_ms(1000);
}

void main()
{
   while(TRUE)
   {
      blink(PIN_B0, 1000);
//!      blink(PIN_B1, 1000);
//!      output_high(PIN_B0);
//!      delay_ms(1000);
//!      output_low(PIN_B0);
//!      delay_ms(1000);
   }
}
