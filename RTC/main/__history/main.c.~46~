#include <main.h>

int hour = 6, minute = 30, second = 32;

void main() {  
   lcd_init();
   lcd_clear();
   
   char string2[16] = "Digital Clock";
   
   lcd_gotoxy(3,1);  // col 6, row 1
   lcd_puts(string2);
   
   while(TRUE) {
      
      
      lcd_gotoxy(5,2);  // col 5, row 2
      lcd_puts(string2);
   }

}
