#include <main.h>

void main()
{  
   char key;
   //col1(1); col2(1); col3(1); col4(1); 
   while(TRUE)
   {  
      key = scan_keypad();
      if (key != -1) {
         printf("Result: %c\r\n", key);
         //while (key != -1)
            //key = scan_keypad();
      delay_ms(500);
      }
   }
}
