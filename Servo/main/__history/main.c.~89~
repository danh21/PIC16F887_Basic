#include <main.h>

void rotateServo(int T_on) {
   output_high(ctrlPulse);
   delay_us(833);
   output_low(ctrlPulse);
   delay_us(20000 - 833);
}

void myDelay(int t) {
   for(int i = 0; i < t; i++)
      delay_us(1);
}

