#include <main.h>

void main() {  
   while(TRUE) {
      rotateServo(0);
      delay_ms(500);
      rotateServo(15);
      delay_ms(500);
      rotateServo(60);
      delay_ms(500);
      rotateServo(75);
      delay_ms(500);
      rotateServo(90);
      delay_ms(500);
      rotateServo(150);
      delay_ms(500);
      rotateServo(180);
      delay_ms(500);
   }
}
