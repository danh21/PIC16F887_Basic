#include <main.h>

void rotateServo(int T_on) {
   output_high(ctrlPulse);
   delay_us(833);
   output_low(ctrlPulse);
   delay_us(20000 - 833);
}

void delay4us(int t) { 
   while(t>0) 
      t--;     
}

void main() {  
   while(TRUE) {
      output_high(ctrlPulse);
      delay4us(Ton);
   output_low(ctrlPulse);
   delay4us(T-Ton);
   }
}
